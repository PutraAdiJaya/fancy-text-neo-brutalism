{
  "name": "fancy-text-neo-brutalism",
  "id": "fancy-text-neo-brutalism",
  "displayName": "Fancy Text Neo-Brutalism",
  "description": "ðŸŽ¨ Transform plain text into 38+ stylish Unicode variants with bold Neo-Brutalism design! Math fonts, decorative text, creative styles. One-click copy & insert.",
  "version": "1.3.0",
  "publisher": "PutraAdiJaya",
  "engines": {
    "vscode": "^1.92.0"
  },
  "icon": "assets/icon-extension.png",
  "categories": [
    "Other",
    "Formatters",
    "Snippets"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "fancyText-container",
          "title": "Fancy Text",
          "icon": "assets/icon.svg"
        }
      ]
    },
    "views": {
      "fancyText-container": [
        {
          "id": "fancyText-view",
          "name": "Styler",
          "type": "webview",
          "icon": "$(symbol-text)"
        }
      ]
    },
    "commands": [
      {
        "command": "fancyText.open",
        "title": "Fancy Text Neo-Brutalism: Focus Sidebar",
        "category": "Fancy Text",
        "icon": "$(symbol-text)"
      },
      {
        "command": "fancyText.insertSelection",
        "title": "Fancy Text Neo-Brutalism: Style Current Selection",
        "category": "Fancy Text",
        "icon": "$(edit)"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "fancyText.insertSelection",
          "group": "navigation@99"
        }
      ]
    }
  },
  "main": "./dist/extension.js",
  "scripts": {
    "compile": "tsc -p .",
    "watch": "tsc -p . -w",
    "prepackage": "npm run compile",
    "package:vsce": "vsce package",
    "publish:vsce": "vsce publish",
    "package:ovsx": "vsce package",
    "publish:ovsx": "ovsx publish",
    "package": "npm run package:vsce",
    "publish": "npm run publish:vsce && npm run publish:ovsx"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "typescript": "^5.5.0",
    "@types/node": "^20.0.0",
    "@vscode/vsce": "^3.0.0",
    "ovsx": "^0.9.2"
  },
  "license": "MIT",
  "keywords": [
    "text",
    "unicode",
    "styling",
    "fancy",
    "neo-brutalism",
    "brutalism",
    "formatting",
    "sidebar",
    "converter",
    "math",
    "symbols",
    "mathematical",
    "script",
    "fraktur",
    "monospace",
    "decorative",
    "emoji",
    "glitch",
    "zalgo",
    "copy",
    "paste",
    "transform",
    "stylish",
    "fonts",
    "typography"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/PutraAdiJaya/fancy-text-neo-brutalism.git"
  },
  "bugs": {
    "url": "https://github.com/PutraAdiJaya/fancy-text-neo-brutalism/issues"
  },
  "homepage": "https://github.com/PutraAdiJaya/fancy-text-neo-brutalism#readme",
  "galleryBanner": {
    "color": "#2D1B69",
    "theme": "dark"
  }
}